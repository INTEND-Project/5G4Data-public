      "targets": [
        {
          "datasource": {
            "type": "sparql",
            "uid": "graphdb"
          },
          "queryText": "PREFIX log: <http://tio.models.tmforum.org/tio/v3.6.0/LogicalOperators/>\nPREFIX icm: <http://tio.models.tmforum.org/tio/v3.6.0/IntentCommonModel/>\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\nPREFIX dct: <http://purl.org/dc/terms/>\nPREFIX data5g: <http://5g4data.eu/5g4data#>\nPREFIX set: <http://tio.models.tmforum.org/tio/v3.6.0/SetOperators/>\nPREFIX quan: <http://tio.models.tmforum.org/tio/v3.6.0/QuantityOntology/>\n\nSELECT DISTINCT \n  (STRAFTER(STR(?intent), \"5g4data#\") AS ?intent_id)\n  (GROUP_CONCAT(DISTINCT ?description; separator=\"; \") AS ?intent_description)\n  (CONCAT(\n    \"http://start5g-1.cs.uit.no:3001/d/fekk4b61d38qof/intent-and-condition-metrics-timeseries-dashboard?var-intent_id=\",\n    STRAFTER(STR(?intent), \"5g4data#\"),\n    \"&var-condition_metrics=\",\n    GROUP_CONCAT(DISTINCT REPLACE(REPLACE(STR(?metric), \".*#\", \"\"), \"-\", \"_\"); separator=\",\")\n  ) AS ?intent_link)\n  (GROUP_CONCAT(DISTINCT ?condition_description; separator=\"; \") AS ?condition_descriptions)\nWHERE {\n  SERVICE <repository:intents_and_intent_reports> {\n    ?intent rdf:type ?intentType ;\n            log:allOf ?expectation .\n\n    ?expectation rdf:type ?expectationType ;\n                 dct:description ?description .\n\n    OPTIONAL {\n      ?expectation log:allOf ?condition .\n      ?condition rdf:type icm:Condition .\n\n      OPTIONAL {\n        ?condition set:forAll [\n          icm:valuesOfTargetProperty ?metric\n        ] .\n      }\n\n      OPTIONAL {\n        ?condition dct:description ?condition_description .\n      }\n    }\n\n    FILTER(CONTAINS(STR(?expectationType), \"Expectation\"))\n  }\n}\nGROUP BY ?intent\nLIMIT 10",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "sparql",
            "uid": "graphdb"
          },
          "queryText": "PREFIX icm: <http://tio.models.tmforum.org/tio/v3.6.0/IntentCommonModel/>\nPREFIX imo: <http://tio.models.tmforum.org/tio/v3.6.0/IntentModelOntology/>\nPREFIX xsd: <http://www.w3.org/2001/XMLSchema#>\n\nSELECT (STRAFTER(STR(?intent), \"5g4data#\") AS ?intent_id)\n       (STRAFTER(STR(?state), \"IntentModelOntology/\") AS ?intent_state)\nWHERE {\n  SERVICE <repository:intents_and_intent_reports> {\n    {\n      SELECT ?intent (MAX(?generated) AS ?latestGenerated)\n      WHERE {\n        ?report a icm:IntentReport ;\n                icm:about ?intent ;\n                icm:reportGenerated ?generated .\n      }\n      GROUP BY ?intent\n    }\n    ?report a icm:IntentReport ;\n            icm:about ?intent ;\n            icm:reportGenerated ?latestGenerated ;\n            icm:intentHandlingState ?state .\n  }\n}",
          "refId": "B"
        }
      ],
      "title": "Intent Details (up to 10 latest, only if state reported, state based on state report)",
